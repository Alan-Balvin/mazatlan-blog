{
  "expo": {
    "name": "mazatlan-blog",
    "slug": "mazatlan-blog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pabloeduardoprogrammer.mazatlanblog"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/palma.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1eea0273-4e01-42d7-83ce-526c4936df67"
      }
    },
    "owner": "pabloeduardoprogrammer",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1eea0273-4e01-42d7-83ce-526c4936df67"
    }
  }
}
